{
  "name": "gitpod-website",
  "version": "2.0.0",
  "private": true,
  "description": "Gitpod.io Website",
  "keywords": [
    "svelte",
    "tailwindcss",
    "gitpod",
    "website"
  ],
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && prettier --check . && svelte-check",
    "check-links": "linkinator --concurrency 1000 --recurse --verbosity=error --skip 'https://(www.)?(notion.so|gitpod.io|github.com|linux.org|twitter.com|www.gitpodstatus.com|researchgate.net|news.ycombinator.com|discord.com|localhost|linkedin|android.com)' --skip '(png|jpg|svg|woff|js|css)$'",
    "cypress:open": "cypress open --browser chrome",
    "deploy": "npm run build",
    "dev": "vite dev --host 0.0.0.0",
    "format": "prettier --write .",
    "generate:adr": "adr new",
    "generate:blog": "plop --plopfile ./generators/index.cjs blog-post",
    "generate:changelog": "node ./scripts/generate-changelog.js",
    "generate:jb": "node ./generators/ide/jb-gen.js && npm run check-links $(gp url 3000)/docs/references/ides-and-editors",
    "prepare": "husky install",
    "start": "vite preview",
    "test:e2e": "cypress run --headless --browser chrome",
    "validate": "svelte-kit sync && svelte-check"
  },
  "lint-staged": {
    "*.{cjs,js,ts,css,scss,md,svelte}": "prettier --write"
  },
  "dependencies": {
    "@lottiefiles/lottie-player": "^1.6.3",
    "@sendgrid/mail": "^7.5.0",
    "@types/rss": "^0.0.30",
    "@types/uuid": "^9.0.1",
    "configcat-js-ssr": "^6.0.1",
    "cookie": "^0.5.0",
    "emoji-regex": "^10.0.0",
    "google-spreadsheet": "^3.1.15",
    "js-cookie": "^3.0.1",
    "nanoid": "^4.0.0",
    "node-fetch": "^3.2.10",
    "rehype-img-size": "^1.0.1",
    "rss": "^1.2.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@octokit/plugin-paginate-graphql": "^2.0.1",
    "@sveltejs/adapter-netlify": "^2.0.7",
    "@sveltejs/kit": "^1.22.1",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/cypress": "^9.0.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/cookie": "^0.5.1",
    "@types/js-cookie": "^3.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/validator": "^13.7.10",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.8",
    "cypress": "^12.17.0",
    "encoding": "^0.1.13",
    "hast-util-heading-rank": "^2.1.0",
    "hastscript": "^7.0.2",
    "husky": "^7.0.4",
    "linkinator": "^4.1.1",
    "lint-staged": "^11.2.6",
    "mdast-util-to-string": "^3.1.0",
    "mdsvex": "^0.11.0",
    "octokit": "^2.0.22",
    "plop": "^3.0.5",
    "postcss": "^8.4.25",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-preset-env": "^9.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "prismjs": "^1.28.0",
    "rehype-wrap-all": "^1.1.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-external-links": "^8.0.0",
    "remark-slug": "^6.0.0",
    "sass": "^1.43.4",
    "svelte": "^3.59.2",
    "svelte-check": "^2.9.2",
    "svelte-focus-trap": "^1.2.0",
    "svelte-lite-youtube-embed": "^1.1.0",
    "svelte-popperjs": "^1.2.8",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "unist-util-visit": "^4.1.0",
    "vite": "^4.4.2"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.19.0"
  }
}
